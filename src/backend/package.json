{
  "name": "tax-optimizer-backend",
  "version": "1.0.0",
  "description": "Backend service for the Low Income Year Tax Optimizer Tool",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "seed": "ts-node prisma/seed.ts"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@supabase/supabase-js": "^2.38.0",
    "@vercel/kv": "^1.0.0",
    "big.js": "^6.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "openai": "^4.0.0",
    "opossum": "^6.0.0",
    "pdfkit": "^3.0.0",
    "pino": "^8.16.0",
    "prom-client": "^14.2.0",
    "rate-limiter-flexible": "^3.0.0",
    "resend": "^1.0.0",
    "uuid": "^9.0.0",
    "yup": "^1.3.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@jest/globals": "^29.0.0",
    "@jest/types": "^29.7.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.7.0",
    "@types/node": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.0"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node prisma/seed.ts"
  },
  "nodemonConfig": {
    "watch": ["src"],
    "ext": ".ts,.js",
    "exec": "ts-node ./src/server.ts"
  }
}